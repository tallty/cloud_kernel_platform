<p id="notice"><%= notice %></p>

<h1>Listing Station Infos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @station_infos.each do |station_info| %>
      <tr>
        <td><%= link_to 'Show', station_info %></td>
        <td><%= link_to 'Edit', edit_station_info_path(station_info) %></td>
        <td><%= link_to 'Destroy', station_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Station info', new_station_info_path %>
